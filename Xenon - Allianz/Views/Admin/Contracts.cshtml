@model IEnumerable<Xenon.Models.ContractModel>

@{
    ViewBag.Title = "Contracts";
}

<h2>Contracts</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Start)
